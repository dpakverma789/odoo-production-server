<?xml version="1.0" encoding="utf-8"?>
<template id="gymwale_dashboard_template" name="Gymwale Dashboard Template">
    <t t-name="gymwale_dashboard_template">
        <div class="container-fluid mt-4 o_dashboard">
            <div class="card mx-5 mt-5">
                <div class="m-5">
                    <input type="date" name="start-date"/>
                    <input type="date" name="end-date"/>
                </div>
                <div class="card-body d-inline-flex">
                    <div class="card col-3 box-shadow m-auto">
                        <div class="card-body">
                            <span class="d-inline-flex">
                                <div class="position-absolute thumbnail" style="background-color:#fcba03">
                                    <i class="fa fa-users icon"/>
                                </div>
                            </span>
                            <span class="number-info" id="total_paid_members_count">--</span>
                        </div>
                        <hr/>
                        <p class="card-tag">Total Paid Members</p>
                    </div>
                    <div class="card col-3 box-shadow m-auto">
                        <div class="card-body">
                            <span class="d-inline-flex">
                                <div class="position-absolute thumbnail" style="background-color:#ff515e">
                                    <i class="fa fa-rupee icon"/>
                                </div>
                            </span>
                            <span class="number-info">&#8377;
                                <span id="total_collection">--</span>
                            </span>
                        </div>
                        <hr/>
                        <p class="card-tag">Total Collection</p>
                    </div>
                    <div class="card col-3 box-shadow m-auto">
                        <div class="card-body">
                            <span class="d-inline-flex">
                                <div class="position-absolute thumbnail" style="background-color:#2dc92c">
                                    <i class="fa fa-frown-o icon"/>
                                </div>
                            </span>
                            <span class="number-info">&#8377;
                                <span id="total_gym_expense">--</span>
                            </span>
                        </div>
                        <hr/>
                        <p class="card-tag">Monthly GYM Expense</p>
                    </div>
                </div>

                <div class="card-body d-inline-flex">
                    <div class="card col-3 box-shadow m-auto">
                        <div class="card-body">
                            <span class="d-inline-flex">
                                <div class="position-absolute thumbnail" style="background-color:#14c1f3">
                                    <i class="fa fa-rupee icon"/>
                                </div>
                            </span>
                            <span class="number-info">&#8377;
                                <span id="monthly_collection">--</span>
                            </span>
                        </div>
                        <hr/>
                        <p class="card-tag">Net Monthly Collection</p>
                    </div>
                    <div class="card col-3 box-shadow m-auto">
                        <div class="card-body">
                            <span class="d-inline-flex">
                                <div class="position-absolute thumbnail" style="background-color:#920ad2">
                                    <i class="fa fa-rupee icon"/>
                                </div>
                            </span>
                             <span class="number-info">&#8377;
                                <span id="net_collection">--</span>
                            </span>
                        </div>
                        <hr/>
                        <p class="card-tag">Net Profit or Loss</p>
                    </div>
                    <div class="card col-3 box-shadow m-auto">
                        <div class="card-body">
                            <span class="d-inline-flex">
                                <div class="position-absolute thumbnail" style="background-color:#8a939b">
                                    <i class="fa fa-rupee icon"/>
                                </div>
                            </span>
                            <span class="number-info">--</span>
                        </div>
                        <hr/>
                        <p class="card-tag">--</p>
                    </div>
                </div>
            </div>
        </div>
    </t>
</template>